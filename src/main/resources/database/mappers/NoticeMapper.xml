<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.winter.app.board.notice.NoticeDAO">

	<select id="getList" resultType="NoticeVO">
		SELECT * FROM NOTICE ORDER BY BOARDNO DESC
	</select>
	
	<insert id="add" parameterType="BoardVO">
		INSERT INTO NOTICE
		VALUES (NULL, #{boardTitle}, #{boardWinter}, #{boardContents}, NOW(), 0)
	</insert>

</mapper>